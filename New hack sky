-----BigButterflies-----

function D1()
  gg.alert('Perminent Until Game Is Restarted')
  gg.setRanges(gg.REGION_CODE_APP)
  gg.setVisible(false)
  gg.searchNumber("0.12",gg.TYPE_FLOAT)
  gg.getResults(200)
  gg.editAll("1",gg.TYPE_FLOAT)
  gg.clearResults()
  end

function D2()
  end

