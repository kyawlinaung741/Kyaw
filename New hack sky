function EXIT()
print("Subscribe To My YouTube Channel Synthiill Gaming")
os.exit()
end


while(true)do
if gg.isVisible(true) then
XGCK=1
gg.setVisible(false)
end
gg.clearResults()
if XGCK==1 then
Main()
end
end  


